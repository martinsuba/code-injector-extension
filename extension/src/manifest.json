{
  "manifest_version": 2,
  "name": "Chrome-Notes",
  "description": "Create description",
  "version": "0.0.1",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/icon48.png"
    },
    "default_title": "Open the popup"
  },
  "content_security_policy": "script-src 'self' 'sha256-IThiKMnsg0UHaLmP7sJxZpd/ohvINImwjxFJyxGFSlk='; object-src 'self'",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "32": "icons/icon32.png",
    "48": "icons/icon48.png"
  }
}